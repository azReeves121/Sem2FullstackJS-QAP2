<!DOCTYPE html>
<html>
  <head>
    <title>Leaderboard</title>
    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <h1>Leaderboard</h1>
    <ul>
      <% topScores.forEach(function(score) { %>
      <li><%= score.streak %> correct answers on <%= score.date %></li>
      <% }); %>
    </ul>
    <a href="/">Return to Home</a>
  </body>
</html>
